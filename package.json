{
  "name": "@tokenring-ai/electron-frontend",
  "description": "TokenRing Electron Frontend",
  "version": "0.2.7",
  "dependencies": {
    "@tokenring-ai/app": "0.2.0",
    "@tokenring-ai/web-host": "0.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "electron": "^40.4.1",
    "electron-builder": "^26.8.1",
    "concurrently": "^9.2.1",
    "vitest": "^4.0.18",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tokenring-ai/monorepo",
    "directory": "frontend/electron"
  },
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "electron:dev": "concurrently \"npm run electron:dev:backend\" \"npm run electron:dev:frontend\"",
    "electron:dev:backend": "bun run build --watch",
    "electron:dev:frontend": "cd ../frontend/chat && bun run dev",
    "electron:build": "npm run build:frontend && npm run build:backend && npm run electron:package",
    "electron:build:all": "npm run build && npm run build:frontend && npm run electron:package",
    "build:frontend": "cd ../frontend/chat && bun run build",
    "build:backend": "bun run build",
    "electron:package": "cd electron && electron-builder",
    "electron:preview": "electron ./dist-electron/win-unpacked/tokenring-coder.exe",
    "electron:package:mac": "cd electron && electron-builder --mac",
    "electron:package:win": "cd electron && electron-builder --win",
    "electron:package:linux": "cd electron && electron-builder --linux",
    "electron:rebuild": "electron-rebuild -f -w better-sqlite3",
    "build": "tsc --noEmit"
  },
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./*": "./*.ts"
  }
}
